<h2>Show check</h2>

<ul>

  <li>
    <strong>Amount:</strong>
    <%= @check.amount %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @check.description %>
  </li>

  <li>
    <strong>Notes:</strong>
    <%= @check.notes %>
  </li>

  <li>
    <strong>Transaction:</strong>
    <%= @check.transaction_id %>
  </li>

</ul>

<%= link "Edit", to: check_path(@conn, :edit, @check) %>
<%= link "Back", to: check_path(@conn, :index) %>
